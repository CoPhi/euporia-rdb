<?xml version="1.0" encoding="UTF-8"?>
<actions>
        <action>
            <actionName>run</actionName>
            <packagings>
                <packaging>jar</packaging>
            </packagings>
            <goals>
                <goal>process-classes</goal>
                <goal>org.codehaus.mojo:exec-maven-plugin:1.2.1:exec</goal>
            </goals>
            <properties>
                <exec.args>-Xms1024m -Xmx2048m -classpath %classpath eu.himeros.euporia.Main euporia25.txt output-x-25.txt</exec.args>
                <exec.executable>java</exec.executable>
                <exec.workingdir>/home/federico/al</exec.workingdir>
            </properties>
        </action>
        <action>
            <actionName>debug</actionName>
            <packagings>
                <packaging>jar</packaging>
            </packagings>
            <goals>
                <goal>process-classes</goal>
                <goal>org.codehaus.mojo:exec-maven-plugin:1.2.1:exec</goal>
            </goals>
            <properties>
                <exec.args>-Xdebug -Xrunjdwp:transport=dt_socket,server=n,address=${jpda.address} -Xms1024m -Xmx2048m -classpath %classpath eu.himeros.euporia.Main euporia25.txt output-x-25.txt</exec.args>
                <exec.executable>java</exec.executable>
                <jpda.listen>true</jpda.listen>
